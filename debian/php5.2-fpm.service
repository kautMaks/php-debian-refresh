[Unit]
Description=The PHP-5.2 FastCGI Process Manager
After=network.target

[Service]
Type=notify
PIDFile=/var/run/php5.2-fpm.pid
ExecStartPre=/usr/lib/php/5.2/php5.2-fpm-checkconf
ExecStart=/usr/sbin/php5.2-fpm --fpm-config /etc/php/5.2/fpm/php-fpm.conf
ExecReload=/bin/kill -USR2 $MAINPID

[Install]
WantedBy=multi-user.target
